float main(			float2 coords	: TEXCOORD0,
			uniform samplerRECT tex1	: TEXUNIT0,
			uniform samplerRECT tex2	: TEXUNIT1,
			uniform samplerRECT tex3	: TEXUNIT2) : COLOR
{
	float val1 = texRECT(tex1, coords);
	float val2 = texRECT(tex2, coords);
	float val3 = texRECT(tex3, coords);
	return val1+val2+val3;
} 
