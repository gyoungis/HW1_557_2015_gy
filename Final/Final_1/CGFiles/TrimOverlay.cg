float4 main(			float2 coords		: TEXCOORD0,
			uniform samplerRECT trimTexture	: TEXUNIT0,
			uniform samplerRECT texture		: TEXUNIT1,
						float4 color		: COLOR0) : COLOR
{
	float trim = texRECT(trimTexture, coords);
	if (trim == 0)
		discard;
	float4 overlay = texRECT(texture, coords);
	return color + overlay;
} 
