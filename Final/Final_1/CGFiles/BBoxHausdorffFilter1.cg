float4 main(float2 coords							: TEXCOORD0,
			uniform samplerRECT minMaxTexture		: TEXUNIT0,
			uniform float minVal									) : COLOR
{
	float4 minMax = texRECT(minMaxTexture, coords);
	float outVal = 1.0;
	if ((minMax.y < minVal) || minMax.x < 0)
		outVal = 0.0;
	return float4(outVal, 1.0, minMax.z, 1.0);
}